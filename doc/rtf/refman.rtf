void mtk::sgels_ (char * {\i trans}, int * {\i m}, int * {\i n}, int * {\i nrhs}, Real * {\i a}, int * {\i lda}, Real * {\i b}, int * {\i ldb}, Real * {\i work}, int * {\i lwork}, int * {\i info})}}
